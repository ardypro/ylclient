#ifndef SERIALHTTP_H_
#define SERIALHTTP_H_

#include "GenericEthernet.h"

namespace yeelink {

class SerialHTTP: public yeelink::GenericEthernet {
public:
	SerialHTTP();
	virtual ~SerialHTTP();

	void setPort(char* port);
	virtual bool connect();
	virtual bool Post(int value);
};

} /* namespace yeelink */
#endif /* SERIALHTTP_H_ */
