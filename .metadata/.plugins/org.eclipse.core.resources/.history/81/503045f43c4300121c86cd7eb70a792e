#ifndef YLGENERICHARDWAREETHERNET_H_
#define YLGENERICHARDWAREETHERNET_H_

#include "ylGenericEthernet.h"

namespace yeelink
{

class ylGenericHardwareEthernet: public yeelink::ylGenericEthernet
{
public:
	ylGenericHardwareEthernet();
	virtual ~ylGenericHardwareEthernet();

	

protected:
	bool connected;
	void virtual connect(void)=0;
	void virtual disconnect()=0;
	

};

} /* namespace yeelink */
#endif /* YLGENERICHARDWAREETHERNET_H_ */
