// Do not remove the include below
#include "yeelink.h"
#include "ylclient.h"

extern HardwareSerial Serial;
using namespace yeelink;

ylclient client;



//The setup function is called once at startup of the sketch
void setup()
{
	Serial.begin(9600);

	client.connect();
}

// The loop function is called in an endless loop
void loop()
{
	client.Post(23);
}
